{
  "contributions": {
    "powerNodes": [
      {
        "name": "generate-qrcode",
        "displayName": "生成二维码",
        "description": "生成录音上传APP用的二维码",
        "inputs": [
          {
            "name": "phone",
            "displayName": "客户电话",
            "bind": {
              "type": "expression",
              "expression": "phone"
            }
          },
          {
            "name": "taskId",
            "displayName": "任务ID",
            "bind": {
              "type": "expression",
              "expression": "task_id"
            }
          }
        ],
        "actions": [
          {
            "name": "refresh",
            "displayName": "刷新二维码",
            "inputs": [
              {
                "name": "phone",
                "type": "expression",
                "expression": "phone"
              },
              {
                "name": "taskId",
                "type": "expression",
                "expression": "task_id"
              }
            ]
          }
        ]
      }
    ]
  },
  "name": "upload-record",
  "version": "0.0.1",
  "displayName": "上传通话录音",
  "description": "配合手机APP实现上传通话录音到任务附件的功能",
  "hookBaseURL": "http://localhost:9999",
  "permissions": ["task:send-message"]
}
